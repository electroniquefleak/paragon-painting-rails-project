<%= form_with url: "/sessions" do |f| %>
    <%= f.label "E-Mail:" %>
    <%= f.email_field :email %><br/>
    <%= f.label "Password:" %>
    <%= f.password_field :password %><br/> 
    <%= f.submit "Login" %>
<% end %>
<%= button_to "Log in with Google", '/auth/google_oauth2'  %>
<div>
    <%= link_to "New user? Register here.", registration_path %>
</div>